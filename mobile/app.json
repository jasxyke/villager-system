{
  "expo": {
    "name": "Villager",
    "scheme": "villager-app",
    "slug": "villager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jasxyke.villager",
      "googleServicesFile": "./google-services.json"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jasxyke.villager",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your fellow residents."
        }
      ],
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "@react-native-firebase/app"
      // "@react-native-firebase/messaging"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5276764e-a82a-4af8-8f8f-d3b4d12266d6"
      }
    }
  }
}
